<div class="card card-body mb-2">
    <h6 class="text-muted">Exam Template</h6>
    <h1>{{examtemplate.title}}</h1>
    <p>{{examtemplate.details}}</p>

    <h5><a href="#" class="badge-pill badge-info">{{examtemplate.type}}</a></h5>

    <hr/>
    <h2 class="text-primary"> Questions </h2>
    {{#each examtemplate.questions}}
        <h3>{{header}}</h3>

        <H6 class="text-success"> Correct </H6>
        {{#each correct}}
            <input type="text" class="form-control is-valid mb-1" value="{{this}}" disabled>
        {{/each}}
        <H6 class="text-danger"> Incorrect </H6>
        {{#each incorrect}}
            <input type="text" class="form-control is-invalid mb-1" value="{{this}}" disabled>
        {{/each}}

        <br/>
    {{/each}}
</div>